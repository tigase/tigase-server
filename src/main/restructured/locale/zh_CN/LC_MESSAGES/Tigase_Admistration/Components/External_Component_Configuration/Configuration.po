msgstr ""

#: ../../Tigase_Admistration/Components/External_Component_Configuration/Configuration.rst:26
msgid "*Domain name* - external component domain name (``muc.devel.tigase.org``)"
msgstr ""

#: ../../Tigase_Admistration/Components/External_Component_Configuration/Configuration.rst:28
msgid ""
"*Domain password* - password for authentication of the external component"
" connection (``muc-pass``)"
msgstr ""

#: ../../Tigase_Admistration/Components/External_Component_Configuration/Configuration.rst:30
msgid ""
"*Connection type* - ``accept`` to make component wait for connection or "
"``connect`` force component to connect to the server (``connect``)"
msgstr ""

#: ../../Tigase_Admistration/Components/External_Component_Configuration/Configuration.rst:32
msgid ""
"*Port number* - port on which component should wait for connection or on "
"which it try to connect (``5270``)"
msgstr ""

#: ../../Tigase_Admistration/Components/External_Component_Configuration/Configuration.rst:34
msgid ""
"*Remote host* - host to connect to (``devel.tigase.org``) *(may be left "
"blank if component will only accept connections)*"
msgstr ""

#: ../../Tigase_Admistration/Components/External_Component_Configuration/Configuration.rst:36
msgid "*Protocol* - id of protocol used for establishing connection"
msgstr ""

